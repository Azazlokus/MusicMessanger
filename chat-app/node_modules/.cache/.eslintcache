\n            default:\r\n                break;\r\n        }\r\n    }\r\n\r\n    return (\r\n        <li className=\"item__input\">\r\n            <span className=\"logIn__item_title\">{props.title}</span>\r\n\r\n            {/* Ошибка для почты */}\r\n            {(loginVoid && loginError) && <div style={{ color: 'red' }}>{loginError}</div>}\r\n            {/* Ошибка для пароля */}\r\n            {(passVoid && passError) && <div style={{color: 'red'}}>{passError}</div>} \r\n            <input id={props.id} onChange={handlerInput} type={props.type} onBlur={blurHandler} placeholder={props.text} className=\"logIn__item_input\" />\r\n        </li>\r\n    );\r\n}\r\n\r\nexport default MyInput;","C:\\Users\\Sofi\\Documents\\GitHub\\MusicMessanger\\chat-app\\src\\Components\\UI\\btns\\myButton.jsx",["97","98"],[],"import React, { useRef, useState } from 'react';\r\n\r\nfunction MyButton(props) {\r\n    return ( \r\n        <button className={props.className} type={props.type}>{props.text}</button>\r\n    );\r\n}\r\n\r\nexport default MyButton;","C:\\Users\\Sofi\\Documents\\GitHub\\MusicMessanger\\chat-app\\src\\Components\\UI\\selectList\\mySelect.jsx",[],[],"C:\\Users\\Sofi\\Documents\\GitHub\\MusicMessanger\\chat-app\\src\\Components\\Log_in\\logIn.jsx",[],[],"C:\\Users\\Sofi\\Documents\\GitHub\\MusicMessanger\\chat-app\\src\\Components\\Registr\\register.jsx",[],[],"C:\\Users\\Sofi\\Documents\\GitHub\\MusicMessanger\\chat-app\\src\\Components\\Chat_Page\\chatPage.jsx",[],[],"C:\\Users\\Sofi\\Documents\\GitHub\\MusicMessanger\\chat-app\\src\\Components\\Chat_Page\\Header\\header.jsx",[],[],"C:\\Users\\Sofi\\Documents\\GitHub\\MusicMessanger\\chat-app\\src\\Components\\Chat_Page\\Local_Page\\localPage.jsx",[],[],"C:\\Users\\Sofi\\Documents\\GitHub\\MusicMessanger\\chat-app\\src\\Components\\Chat_Page\\ChatList\\chatList.jsx",[],[],"C:\\Users\\Sofi\\Documents\\GitHub\\MusicMessanger\\chat-app\\src\\Components\\UI\\btns\\myChatCreateButton.jsx",[],[],"C:\\Users\\Sofi\\Doc